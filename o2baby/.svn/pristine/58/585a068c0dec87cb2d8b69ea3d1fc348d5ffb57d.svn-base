/* my-deposit.wxss */
@import "../tpls/dialog/dialog.wxss";
.container{
  position: absolute;
  top:0;
  right:0;
  bottom: 0;
  left:0;
  display: flex;
  min-height: 100%;
  max-height: 100%;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.back-word {
  position: absolute;
  top:14.2%;
  display: block;
  font-size: 24rpx;
  color: #b3acff;
  text-align: center;
}

.back-num {
  position:absolute;
  top:18.5%;
  display: block;
  height: 140rpx;
  margin-top: 18rpx;
  font-size: 100rpx;
  color: #9eac4b;
  text-align: center;
}

.btn-area {
  position:absolute;
  top:50%;
  left:10%;
  right:10%;
}

.back-money {
  font-size: 36rpx;
  text-align: center;
  color: #879633;
  width: 100%;
  height: 94rpx;
  background-color: transparent;
  border-radius: 200rpx;
  box-shadow: 0 6rpx 12rpx 2rpx rgba(197, 211, 117, 0.33);
  border: solid 1px #879633;
}
.back-money:active{
  opacity: 0.6;
}

.back-money::after {
  border: none;
}

